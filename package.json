{
  "name": "dOrg-dApp",
  "version": "0.1.0",
  "scripts": {
    "start": "concurrently \"npm:eth-start\" \"npm:app-start\"",
    "app-start": "react-scripts start",
    "app-build": "react-scripts build && echo 'dorg.tech' > build/CNAME",
    "app-eject": "react-scripts eject",
    "app-test": "react-scripts test",
    "eth-start": "concurrently \"npm:eth-ganache-start\" \"npm:eth-arc-migrate\"",
    "eth-ganache-start": "npm explore @daostack/arc.js -- npm start ganache",
    "eth-arc-migrate": "wait-on tcp:8545 && npm explore @daostack/arc.js -- npm start migrateContracts"
  },
  "dependencies": {
    "@daostack/arc.js": "0.0.0-alpha.89",
    "@material-ui/core": "^3.2.2",
    "@material-ui/icons": "^3.0.1",
    "@types/jest": "^23.3.10",
    "@types/node": "^10.12.18",
    "@types/recharts": "^1.1.6",
    "bignumber.js": "^5.0.0",
    "connected-react-router": "^4.5.0",
    "ethereum-blockies-png": "^0.1.3",
    "history": "^4.7.2",
    "material-icons": "^0.2.3",
    "ramda": "^0.25.0",
    "react": "^16.5.2",
    "react-blockies": "^1.4.0",
    "react-dom": "^16.5.2",
    "react-particles-js": "^2.4.0",
    "react-redux": "^5.0.7",
    "react-router": "^4.3.1",
    "recharts": "^1.4.2",
    "redux": "^4.0.1",
    "redux-thunk": "^2.3.0",
    "web3": "^0.20.4"
  },
  "devDependencies": {
    "@types/bignumber.js": "^5.0.0",
    "@types/history": "^4.7.2",
    "@types/ramda": "^0.25.39",
    "@types/react": "^16.7.18",
    "@types/react-dom": "^16.0.11",
    "@types/react-redux": "^6.0.9",
    "@types/react-router": "^4.0.32",
    "@types/redux": "^3.6.0",
    "@types/redux-thunk": "^2.1.0",
    "@types/web3": "^1.0.14",
    "concurrently": "^4.1.0",
    "husky": "^1.1.2",
    "prettier": "^1.15.2",
    "pretty-quick": "^1.8.0",
    "react-scripts": "^2.1.2",
    "typescript": "^3.2.2",
    "wait-on": "^3.2.0",
    "webpack": "4.19.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
